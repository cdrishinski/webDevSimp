<h2>Search Books</h2>
<form action="/books" method="GET">
    <label for="name">Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<br>
<% books.forEach(book => { %>
<div><%= book.title %></div>    
<div><%= book.author %></div>    
<% })%>